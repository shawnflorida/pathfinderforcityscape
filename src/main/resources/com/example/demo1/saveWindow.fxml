<AnchorPane prefHeight="384.0" prefWidth="411.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.newui.SaveWindowController">
   <children>
      <Text fx:id="saveFileName" layoutX="59.0" layoutY="129.0" strokeType="OUTSIDE" strokeWidth="0.0" text="File Name">
         <font>
            <Font name="Tw Cen MT" size="14.0" />
         </font>
      </Text>
      <TextField fx:id="saveTextField" layoutX="59.0" layoutY="140.0" prefHeight="30.0" prefWidth="293.0" promptText="Enter Text" />
      <Text fx:id="saveFileTypeLabel" layoutX="59.0" layoutY="215.0" strokeType="OUTSIDE" strokeWidth="0.0" text="File Type">
         <font>
            <Font name="Tw Cen MT" size="14.0" />
         </font>
      </Text>
      <ComboBox fx:id="saveComboBox" layoutX="58.0" layoutY="227.0" prefHeight="30.0" prefWidth="294.0" promptText="Choose File Type" />
      <Button fx:id="saveButton" layoutX="253.0" layoutY="330.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" style="-fx-background-color: gray; -fx-background-radius: 30;" text="Save" textFill="WHITE">
         <font>
            <Font name="Tw Cen MT" size="14.0" />
         </font>
      </Button>
      <Button fx:id="saveCancelButton" layoutX="59.0" layoutY="330.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" style="-fx-background-color: transparent;" text="Cancel">
         <font>
            <Font name="Tw Cen MT" size="14.0" />
         </font>
      </Button>
      <Text fx:id="saveProjectLabel" layoutX="59.0" layoutY="71.0" strokeType="OUTSIDE" strokeWidth="0.0" text="save Project">
         <font>
            <Font name="Perpetua Titling MT Bold" size="20.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
