<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="imageAnchor" prefHeight="692.0" prefWidth="1179.0" style="-fx-background-color: #F1DBBF;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.imageController">
   <children>
      <TilePane fx:id="tilePane" alignment="CENTER" layoutX="24.0" layoutY="51.0" prefHeight="601.0" prefWidth="1132.0" style="-fx-background-color: #F1DBBF;">
         <effect>
            <Glow />
         </effect></TilePane>
      <Button fx:id="add1" layoutX="917.0" layoutY="661.0" mnemonicParsing="false" onAction="#addButtonAction" prefHeight="25.0" prefWidth="113.0" text="Find Images" />
      <Button fx:id="clear" layoutX="1038.0" layoutY="661.0" mnemonicParsing="false" onAction="#clearImages" prefHeight="25.0" prefWidth="113.0" text="Clear" />
      <Label layoutX="24.0" layoutY="14.0" prefHeight="27.0" prefWidth="738.0" text="Instruction: Choose a folder and click on an image to be added into the Tiles. This only accepts png files." textAlignment="RIGHT">
         <font>
            <Font size="16.0" />
         </font>
         <effect>
            <Blend />
         </effect>
      </Label>
   </children>
</AnchorPane>
